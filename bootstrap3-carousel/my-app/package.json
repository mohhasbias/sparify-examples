{
  "name": "sparify",
  "version": "1.0.0",
  "description": "Boilerplate for single page application powered by browserify",
  "main": "app/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "budo app/app.js:dist/bundle.js --live --open --pushstate",
    "build": "browserify app/app.js -o dist/bundle.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "browserify-postcss": "^0.3.3",
    "browserify-shim": "^3.8.14",
    "budo": "^9.4.7",
    "postcss-import": "^9.1.0",
    "postcss-modules": "^0.6.4",
    "postcss-url": "^5.1.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "diffhtml": "^0.9.2",
    "jquery": "^3.2.1",
    "page": "^1.7.1",
    "yo-yo": "^1.4.0"
  },
  "browserify": {
    "transform": [
      [
        "browserify-postcss",
        {
          "plugin": [
            "postcss-url",
            [
              "autoprefixer",
              {
                "browsers": [
                  "last 2 versions",
                  "ie 9"
                ]
              }
            ]
          ],
          "inject": true
        }
      ],
      "browserify-shim"
    ]
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
